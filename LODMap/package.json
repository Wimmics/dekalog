{
   "name": "kartographi",
   "source": "src/index.html",
   "scripts": {
      "build": "npx tsc",
      "predeploy": "rm dist/* ./*.js ./*.js.map ./*.css.map ./*.css || true",
      "deploy": "npx parcel build",
      "postdeploy": "sed 's/\\/index\\./\\.\\/index\\./g' dist/index.html > index.html && rm dist/index.html || true && mv dist/* .",
      "predev": "rm dist/* || true && rm dist/data/* || true && cp ../KartoGraphICaching/data/runSets.json ../KartoGraphICaching/data/timezoneMap.json ../KartoGraphICaching/data/SPARQLFeatureDescriptions.json dist/data && cp ../KartoGraphICaching/data/cache/* dist/data  || true",
      "dev": "npx parcel serve --https"
   },
   "devDependencies": {
      "@types/node-fetch": "^2.6.2",
      "crypto-browserify": "^3.12.0",
      "dayjs": "latest",
      "echarts": "^5.4.2",
      "jquery": "^3.6.4",
      "leaflet": "latest",
      "parcel": "^2.3.1",
      "punycode": "^1.4.1",
      "querystring-es3": "^0.2.1",
      "stream-browserify": "^3.0.0",
      "url": "^0.11.0"
   },
   "dependencies": {
      "@types/bootstrap": "^5.2.6",
      "@types/echarts": "^4.9.16",
      "@types/jquery": "^3.5.16",
      "@types/leaflet": "^1.9.3",
      "@types/md5": "latest",
      "@types/node": "^18.11.9",
      "@types/uuid": "^9.0.0",
      "buffer": "^6.0.3",
      "echart": "^0.1.3",
      "events": "^3.3.0",
      "fetch": "^1.1.0",
      "graphy": "^4.3.4",
      "gridjs": "^6.0.6",
      "gridjs-jquery": "^4.0.0",
      "md5": "^2.3.0",
      "process": "^0.11.10",
      "typescript": "^4.9.5",
      "xmlhttprequest": "^1.8.0"
   }
}
