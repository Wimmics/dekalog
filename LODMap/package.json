{
   "name": "KartoGraphI",
   "source": "src/index.html",
   "scripts": {
      "build": "npx tsc",
      "predeploy": "rm dist/* ./*.js ./*.js.map ./*.css.map ./*.css || true",
      "deploy": "npx parcel build",
      "postdeploy": "sed 's/\\/index\\./\\.\\/index\\./g' dist/index.html > index.html && rm dist/index.html || true && mv dist/* .",
      "dev": "npx parcel serve --https",
      "predev": "rm dist/* || true"
   },
   "devDependencies": {
      "@types/node-fetch": "^2.6.2",
      "crypto-browserify": "^3.12.0",
      "dayjs": "latest",
      "echarts": "^5.4.2",
      "jquery": "latest",
      "leaflet": "latest",
      "parcel": "^2.3.1",
      "stream-browserify": "^3.0.0"
   },
   "dependencies": {
      "@types/bootstrap": "^5.2.6",
      "@types/echarts": "^4.9.16",
      "@types/jquery": "^3.5.16",
      "@types/md5": "latest",
      "@types/node": "^18.11.9",
      "@types/uuid": "^9.0.0",
      "buffer": "^6.0.3",
      "datatables.net": "^1.11.5",
      "datatables.net-bs5": "^1.11.5",
      "datatables.net-dt": "^1.11.5",
      "events": "^3.3.0",
      "fetch": "^1.1.0",
      "graphy": "^4.3.4",
      "md5": "^2.3.0",
      "process": "^0.11.10",
      "typescript": "^4.9.5",
      "xmlhttprequest": "^1.8.0"
   }
}
