@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix dkg: <https://dekalog.univ-nantes.fr/> .
@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .

dkg:missingValue a rdfs:Thing ;
    rdfs:label "Placeholder"@en ;
    rdfs:comment "Placeholder for the missing values of a property in a dataset or endpoint description" .

dkg:featureProperty a rdfs:Property ;
    rdfs:domain earl:Assertion ;
    rdfs:range rdf:Property ;
    rdfs:label "Values of the feature" ;
    rdfs:comment "Used on EARL assertions to indicate which feature of the dataset of endpoint description should have used the results of the test" .
