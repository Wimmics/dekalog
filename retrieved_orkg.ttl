@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ns1:	<http://localhost:8890/> .
@prefix sd:	<http://www.w3.org/ns/sparql-service-description#> .
@prefix ns3:	<http://www.w3.org/ns/formats/> .

ns1:sparql
	rdf:type	sd:Service ;
	sd:endpoint	ns1:sparql ;
	sd:feature	sd:UnionDefaultGraph , sd:DereferencesURIs ;
	sd:resultFormat	ns3:RDFa , ns3:N-Triples , ns3:N3 , ns3:RDF_XML , ns3:SPARQL_Results_CSV , ns3:SPARQL_Results_JSON , ns3:SPARQL_Results_XML , ns3:Turtle ;
	sd:supportedLanguage	sd:SPARQL10Query ;
	sd:url	ns1:sparql .
