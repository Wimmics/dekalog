@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<> a kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "There are as many classes as expected."@en ;
    kgi:timeout "PT120S"^^xsd:duration ;
    kgi:query """
        PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
        PREFIX owl: <http://www.w3.org/2002/07/owl#>
        PREFIX void: <http://rdfs.org/ns/void#>
        PREFIX dcat: <http://www.w3.org/ns/dcat#>
        ASK {
                ?res void:classes ?extractedValue .
                ?res ?p $endpointUrl .
                { ?res a dcat:Dataset }
                UNION { ?res a void:Dataset }
                {
                    SELECT (count( DISTINCT ?c) AS ?value)
                    WHERE {
                        ?s a ?c .
                    }
                }
                FILTER(?extractedValue != ?value)
        } """ .
