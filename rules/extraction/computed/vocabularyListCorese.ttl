@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "Extraction of the namespaces of properties and classes."@en ;
    kgi:endpoint kgi:federation ;
    kgi:query """ASK
        WHERE {
            SERVICE $endpointUrl {
                { SELECT DISTINCT ?elem { { ?s ?elem ?o . }
                     UNION { ?s a ?elem . }
                } }
        }
        BIND(REPLACE( str(?elem), "(#|/)[^#/]*$", "$1") AS ?ns) .
        }""" .
