@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<> a kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "There are as many properties as expected."@en ;
    kgi:query """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
            PREFIX owl: <http://www.w3.org/2002/07/owl#>
            PREFIX void: <http://rdfs.org/ns/void#>
            PREFIX dcat: <http://www.w3.org/ns/dcat#>
            ASK {
                ?res void:properties ?extractedValue .
                ?res ?p $endpointUrl .
                { ?res a dcat:Dataset }
                UNION { ?res a void:Dataset }
                {
                    SELECT (count(DISTINCT ?p) AS ?value) WHERE {
                        ?s ?p ?o .
                    }
                }
                FILTER ( ?value != ?extractedValue )
            } """ .
