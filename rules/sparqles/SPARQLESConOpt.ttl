@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

kgi:ConOptTest rdf:type kgi:TestQuery ;
    dcterms:title "Optional in construct" ;
    dcterms:description "SPARQL CONSTRUCT query with two triple patterns and an optional returning an RDF graph of resources and their types" ;
    kgi:query """PREFIX nonsensical: <http://nonsensic.al> PREFIX ex: <http://e.g> CONSTRUCT { ?x ex:type ?v } WHERE { nonsensical:1 ex:type ?o . OPTIONAL {?o ex:type ?v } } LIMIT 10"""  .
