@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix dkg: <http://ns.inria.fr/kgindex#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

dkg:classCountExtractionTest rdf:type dkg:TestQuery , earl:TestCase ;
    dcterms:title "Count of the classes in the dataset"@en ;
    dcterms:requires dkg:reachableEndpointTest ;
    dkg:query """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
        PREFIX owl: <http://www.w3.org/2002/07/owl#>
        SELECT (count(?class) AS ?count)
        WHERE {
            SELECT DISTINCT ?class
            $FROM
            WHERE {
                { ?class a owl:Class }
                UNION { ?class a rdfs:Class }
                UNION { ?whatever a ?class }
            }
        }""" .
